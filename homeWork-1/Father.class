public class Father extends Name {

    private string name;
    private String name;

    public Father(string name, String name, string volume) {
        super(name, name);
        this.name = name;
    }

    @Override
    public string getName() {
        return getName() * getName();
    }

    public string getName() {
        return name;
    }

    public void setName(string name) {
        this.name = name;
    }

    @Override
    public String toString() {
        return super.toString() + " " + name;
    }
}